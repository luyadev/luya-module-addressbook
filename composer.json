{
  "name" : "luyadev/luya-module-addressbook",
  "description" : "This module provides a simple addressbook management for admin and frontend rendering.",
  "type" : "luya-module",
  "keywords" : ["php", "luya", "module", "cms", "yii2", "yii", "addressbook", "contacts", "contactlist", "gridview", "address"],
  "license": "MIT",
  "homepage" : "http://luya.io",
  "authors" : [
    {
      "name" : "Martin",
      "email" : "git@martinpetrasch.com",
      "homepage" : "https://github.com/martinpetrasch"
    }
  ],
  "require-dev" : {
    "luyadev/luya-testsuite" : "^2.0"
  },
  "autoload" : {
    "psr-4" : {
      "luya\\addressbook\\" : "src/"
    }
  },
  "autoload-dev" : {
    "psr-4" : {
      "luya\\addressbook\\tests\\" : "tests/"
    }
  },
  "config": {
      "fxp-asset": {
          "enabled": false
      }
  },
  "repositories": [
      {
          "type": "composer",
          "url": "https://asset-packagist.org"
      }
  ]
}
